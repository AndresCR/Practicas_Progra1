        N=Integer.parseInt(teclado.nextLine());              
        System.out.print("Digite el valor de A" +"\n");
        A=Double.parseDouble(teclado.nextLine());
        System.out.println("Digite el valor de C" +"\n"); 
        int asciiValue=(int)CharacterValue;
        System.out.println("asciiValue");
                
        System.out.print("El valor de N es" + N);
        System.out.print("El valor de A es" + A);
        System.out.print("El valor de C es" + C);
        
        x=N+A;
        System.out.println("La suma de N+A es"+ x);
        
        y=A-N;
        System.out.println("La diferencia de A-B es"+ y);
